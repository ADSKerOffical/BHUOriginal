do
  if not workspace:FindFirstChild("Madness sphere") then
local madness = Instance.new("Part", workspace)
madness.Anchored = true
madness.Name = "Madness sphere"
madness.Size = Vector3.new(2, 2, 2)
madness.Color = Color3.new(1, 0, 0)
madness.Shape = "Ball"
madness.Material = "Neon"
madness.Position = Vector3.new(538, 6, -552)

for _, side in Enum.NormalId:GetEnumItems() do
  local decal = Instance.new("Decal", madness)
  decal.Texture = "rbxassetid://10532241402"
  decal.Face = side
end

local ambient = Instance.new("Sound", madness)
ambient.SoundId = "rbxassetid://78050074366725"
ambient.Looped = true
ambient:Play()

local particle = Instance.new("ParticleEmitter", madness)
particle.Texture = "rbxassetid://115034218145529"
particle.Speed = NumberRange.new(0, 0)
particle.Rate = 15
particle.Size = NumberSequence.new(1.5, 0)
particle.Lifetime = NumberRange.new(0.25, 0.25)

local col2 = Instance.new("ColorCorrectionEffect", game.Lighting)
col2.Enabled = false
col2.Saturation = 100
col2.TintColor = Color3.new(1, 0, 0)

pcall(function() game.Lighting:FindFirstChildOfClass("Sky"):Destroy() end)
local sky = Instance.new("Sky")
sky.CelestialBodiesShown = false
sky.SkyboxBk = "rbxassetid://98837168684167"
sky.SkyboxDn = "rbxassetid://98837168684167"
sky.SkyboxFt = "rbxassetid://98837168684167"
sky.SkyboxLf = "rbxassetid://98837168684167"
sky.SkyboxRt = "rbxassetid://98837168684167"
sky.SkyboxUp = "rbxassetid://98837168684167"

local ambient = Instance.new("Sound", workspace)
ambient.SoundId = "rbxassetid://9116732970"
ambient.Looped = true

local baseplate = Instance.new("Part", workspace)
baseplate.Name = "THERE IS NO SALVATION" 
baseplate.Anchored = true
baseplate.Color = Color3.new(1, 1, 1)
baseplate.Size = Vector3.new(2048, 2, 2048)
baseplate.Position = Vector3.new(0, 10000, 0)

coroutine.wrap(function()
  while task.wait() do
    local hrp = (game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()):FindFirstChild("HumanoidRootPart")
    if hrp then
      baseplate.Position = Vector3.new(hrp.CFrame.X, 10000, hrp.CFrame.Z)
    end
  end
end)()

    madness.Touched:Connect(function(hit)
      if hit:IsDescendantOf(game.Players.LocalPlayer.Character) then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = baseplate.CFrame * CFrame.new(0, 3, 0)
        pcall(function() game.CoreGui.RobloxGui.Enabled = false end)
        col2.Enabled = true
        sky.Parent = game.Lighting
        ambient.Playing = true 
      end
    end)
  end
end
